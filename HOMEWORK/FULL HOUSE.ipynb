   "source": [
    "import turtle\n",
    "pen = turtle.Turtle()\n",
    "turtle.bgcolor(\"skyblue\")\n",
    "for i in range(4) :\n",
    "    pen.forward(100)\n",
    "    pen.right(90)\n",
    "pen.left(45)\n",
    "import math\n",
    "x = math.sin(45)\n",
    "pen.forward(100*x)\n",
    "pen.right(90)\n",
    "pen.forward(100*x)\n",
    "turtle.done()\n"
   ]
